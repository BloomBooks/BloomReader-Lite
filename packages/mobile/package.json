{
    "name": "bloom-reader-lite-mobile",
    "version": "1.0.0",
    "license": "MIT",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "tsc && expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "build:ios": "yarn && eas build --platform ios --profile development",
        "build:ios:prod": "yarn && eas build --platform ios --profile production",
        "postinstall": "yarn copy-web-dist && yarn copy-autogenerated",
        "copy-web-dist": "cpx \"./node_modules/bloom-reader-lite-web/dist/**/*\" ./assets/web-dist -v --clean",
        "copy-autogenerated": "cpx \"./node_modules/bloom-reader-lite-web/output/autogenerated/mobile/*.*\" ./src/autogenerated -v --clean"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "1.18.2",
        "bloom-reader-lite-shared": "link:./../shared",
        "expo": "~49.0.5",
        "expo-asset": "~8.10.1",
        "expo-dev-client": "~2.4.6",
        "expo-linking": "~5.0.2",
        "expo-splash-screen": "~0.20.4",
        "expo-status-bar": "~1.6.0",
        "react": "18.2.0",
        "react-native": "0.72.3",
        "react-native-extended-stylesheet": "^0.12.0",
        "react-native-root-toast": "^3.4.1",
        "react-native-url-polyfill": "^2.0.0",
        "react-native-webview": "13.2.2",
        "react-native-zip-archive": "^6.0.9"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/react": "~18.0.14",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "bloom-reader-lite-web": "link:./../web",
        "cpx": "^1.5.0",
        "eslint": "^8.48.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "typescript": "^5.1.3"
    },
    "packageManager": "yarn@1.22.19",
    "volta": {
        "node": "16.14.0",
        "yarn": "1.22.19"
    },
    "private": true
}
